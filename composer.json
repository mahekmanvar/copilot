{
  "name": "guesl/copilot",
  "description": "Guesl wrapper card connect payment gateway.",
  "keywords": [
    "CardConnect",
    "Payment Gateway",
    "Laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jianfeng Li",
      "email": "modifyljf@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/contracts": "~5.4",
    "illuminate/console": "~5.1",
    "illuminate/http": "~5.4",
    "illuminate/support": "~5.4"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "psr-4": {
      "Guesl\\CardConnect\\CoPilot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guesl\\CardConnect\\CoPilot\\CoPilotServiceProvider"
      ],
      "aliases": {
        "Debugbar": "Guesl\\CardConnect\\CoPilot\\Facdeds\\CoPilot"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}